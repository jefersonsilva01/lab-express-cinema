<div id="movies-container">
  <h1>Ironhack Cinema</h1>
  <p>Click on the movie to check the showtime!</p>
  <div id="movies">
    {{#each movies}}
      <div class="movie">
        <img src="{{this.image}}" alt="Movie Cover" />
        <Strong>{{this.title}}</Strong>
        <a type="button" class="btn btn-primary" href="/movie/{{this.id}}">See
          more</a>
      </div>
    {{/each}}
  </div>
</div>